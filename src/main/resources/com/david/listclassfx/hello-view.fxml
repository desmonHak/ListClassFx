<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="rootVbox" alignment="CENTER" prefHeight="454.0" prefWidth="1129.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.david.listclassfx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TableView fx:id="tablaDatos" editable="true" prefHeight="246.0" prefWidth="666.0" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="ColConstructores" prefWidth="193.0" text="Constructores" />
            <TableColumn fx:id="ColMethods" minWidth="0.0" prefWidth="170.0" text="Methods" />
            <TableColumn fx:id="ColAtributos" minWidth="0.0" prefWidth="206.0" text="Atributos" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>

    <Label fx:id="welcomeText" />
    <AnchorPane fx:id="anchoPanel" prefHeight="85.0" prefWidth="668.0">
        <children>
            <TextField fx:id="CampoClassBuscar" prefHeight="29.0" prefWidth="371.0" />
            <Button fx:id="BotonBuscar" layoutX="395.0" layoutY="2.0" mnemonicParsing="false" onAction="#onActionBotonBuscar" text="Buscar" />
         <Button fx:id="Limpiar" layoutX="459.0" layoutY="2.0" mnemonicParsing="false" onAction="#OnActionLimpiar" text="Limpiar" />
        </children>
    </AnchorPane>
</VBox>
